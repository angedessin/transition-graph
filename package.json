{
  "name": "transition-graph",
  "version": "1.0.0",
  "repository": "git@github.com:angedessin/transition-graph.git",
  "license": "MIT",
  "volta": {
    "node": "20.18.1",
    "pnpm": "9.15.0"
  },
  "browserslist": [
    "> 0.2%, not dead",
    "not IE 11",
    "Safari > 16, iOS > 16"
  ],
  "scripts": {
    "prepare": "rimraf .next && rimraf .config && husky",
    "predev": "rimraf .next",
    "dev": "next dev --experimental-https",
    "build": "next build && next-image-export-optimizer",
    "build:test": "next build",
    "analyze": "cross-env ANALYZE=true pnpm start",
    "start": "next build && next start -p 8888",
    "preview": "pnpm build && pnpm http-server out",
    "clean": "rimraf .next out",
    "lint": "run-s fix:* && pnpm prettier",
    "fix:stylelint": "stylelint --fix '**/*.scss' && prettier --write '**/*.scss'",
    "fix:eslint": "eslint --fix './**/*.{ts,tsx}' && prettier --write '**/*.{ts,tsx}'",
    "prettier": "prettier --write '**/*.{js,mjs,json,css,scss}'",
    "svgr": "run-s svgr:*",
    "svgr:icon": "svgo -f src/app/_ui/icon/assets --config ./svgo.config.js && svgr --filename-case kebab src/app/_ui/icon/assets/*.svg --ext tsx --out-dir src/app/_ui/icon",
    "test": "jest",
    "check-types": "tsc --noEmit --pretty"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.4",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.16.0",
    "@next/bundle-analyzer": "14.2.20",
    "@svgr/cli": "8.1.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@types/eslint__js": "8.42.3",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.1",
    "@types/react": "18.3.14",
    "@types/react-dom": "18.3.2",
    "@types/testing-library__jest-dom": "5.14.9",
    "@types/ua-parser-js": "0.7.39",
    "@typescript-eslint/parser": "8.17.0",
    "autoprefixer": "10.4.20",
    "babel-jest": "29.7.0",
    "browserslist": "4.24.2",
    "cpx2": "8.0.0",
    "cross-env": "7.0.3",
    "eslint": "9.16.0",
    "eslint-config-next": "14.2.20",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-typescript-sort-keys": "3.3.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "15.13.0",
    "http-server": "14.1.1",
    "husky": "9.1.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-fail-on-console": "3.3.1",
    "lint-staged": "15.2.10",
    "next-transpile-modules": "10.0.1",
    "npm-run-all2": "7.0.1",
    "postcss": "8.4.49",
    "postcss-preset-env": "10.1.1",
    "postcss-sorting": "9.1.0",
    "prettier": "3.4.2",
    "rimraf": "6.0.1",
    "sass": "1.82.0",
    "stylelint": "16.11.0",
    "stylelint-config-recommended": "14.0.1",
    "stylelint-config-recommended-scss": "14.1.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-declaration-block-no-ignored-properties": "2.8.0",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.10.0",
    "svgo": "3.3.2",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.17.0",
    "webpack": "5.97.1"
  },
  "dependencies": {
    "@next/third-parties": "14.2.20",
    "clsx": "2.1.1",
    "destyle.css": "4.0.1",
    "jotai": "2.10.3",
    "jotai-devtools": "0.10.1",
    "next": "14.2.20",
    "next-image-export-optimizer": "1.18.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.1.2",
    "ua-parser-js": "2.0.0"
  }
}
